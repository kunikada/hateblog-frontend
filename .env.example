# API Base URL (開発時は /api/v1 でプロキシ経由)
VITE_API_BASE_URL=/api/v1

# プロキシ先 (Vite dev serverが使用)
VITE_API_PROXY_TARGET=http://localhost:8000


# Enable MSW (Mock Service Worker) for API mocking
# Set to 'true' to use mocked API responses, 'false' to use real API
VITE_ENABLE_MSW=false

# Pagination
VITE_ENTRIES_PER_PAGE=25

# Google Tag Manager
VITE_GTM_ID=
