{
  "name": "hateblog-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "test": "vitest run",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "openapi:generate": "openapi-generator-cli generate -i openapi.yaml -g typescript-fetch -o src/api/gen --additional-properties=supportsES6=true,withInterfaces=true,useSingleRequestParameter=true"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^5.40.1",
    "@tanstack/react-router": "^1.42.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@tanstack/router-devtools": "^1.42.0",
    "@tanstack/router-vite-plugin": "^1.42.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "msw": "^2.4.2",
    "npm-check": "^6.0.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.5.4",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  }
}
